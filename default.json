{
    "extends": [
      "config:base"
    ],
    "labels": [
      "renovate"
    ],
    "packageRules": [
      {
        "updateTypes": ["minor", "patch", "digest"],
        "matchPackagePatterns": [
          "^org.apache.maven.plugins:maven*",
          "^org.codehaus.mojo:exec-maven-plugin*",
          "^github.com/aws/aws-sdk-go",
          "^ch.qos.logback:logback-core*",    
          "^ch.qos.logback:logback-classic*",
          "^com.amazonaws:aws-*",
          "^org.slf4j:slf4j*",  
          "^org.slf4j:jcl*",  
          "^org.slf4j:jul*",  
          "^org.mockito:mockito*",  
          "^org.flywaydb:flyway*",  
          "^com.fasterxml.jackson.dataformat:jackson*", 
          "^com.fasterxml.jackson.datatype:jackson*",  
          "^com.fasterxml.jackson.module:jackson*",  
          "^com.fasterxml.jackson.core:jackson*",
          "^com.slack.api:slack-api-client*",  
          "^org.apache.httpcomponents:httpclient*",  
          "^org.apache.httpcomponents:fluent",    
          "^no.cantara.config:property-config",
          "^no.cantara.base:Resilience4j-*",  
          "^no.cantara.base:Hystrix-*",   
          "^no.cantara.jau:configservice*",  
          "^no.cantara.config:simple-microservice-config*",  
          "^org.valuereporter:valuereporter*",  
          "^no.cantara.messi:messi-*",	  
          "^no.cantara.stingray:stingray*",  
          "^net.whydah.sso:Whydah-*",
          "^software.amazon.awssdk:ses",
          "^org.yaml:snakeyaml",  
          "^ch.qos.logback:logback-classic*",  
          "^ch.qos.logback:logback*",  
          "^no.cantara.config:simple-microservice-config",  
          "^no.entra.bacnet:bacnet*",  
          "^no.entra.entraos.api:realestate*",  
          "^io.swagger.core.v3:swagger*",  
          "^org.junit.jupiter:junit*",  
          "^software.amazon.awssdk:bom",                    
          "^com.exoreaction.catalystone:event-*",  
          "^com.exoreaction.xorcery:grpc*",  
          "^com.exoreaction.xorcery:xorcery*"
        ],
        "automerge": true,
        "ignoreTests": true
      }
    ]
  }
